{
  "compilerOptions": {
    "target": "es5",                                  
    "module": "commonjs",                                
    "moduleResolution": "node",                       
    "sourceMap": true,                                
    "outDir": "./dist",                                  
    "removeComments": true,                           
    "esModuleInterop": true,                             
    "forceConsistentCasingInFileNames": true,           
    "strict": true,                                      
    "skipLibCheck": true                                 
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules","dist"]
}
